<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         version="2.5" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>submitForm</servlet-name>
		<servlet-class>com.sai.helloworld.SubmitFormServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>submitForm</servlet-name>
		<url-pattern>/submitForm</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>com.sai.helloworld.HelloWorldServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Employee</servlet-name>
		<servlet-class>com.sai.helloworld.EmployeeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Employee</servlet-name>
		<url-pattern>/employees</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>EditEmployee</servlet-name>
		<servlet-class>com.sai.helloworld.EditEmployeeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>EditEmployee</servlet-name>
		<url-pattern>/EditEmployee</url-pattern>
	</servlet-mapping>

</web-app>
